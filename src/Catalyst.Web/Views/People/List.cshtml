@model IEnumerable<Catalyst.Core.Models.Domain.IPerson>

<h3>List people here</h3>
<ul>
    <li><a href="/person/rusty/">Rusty</a></li>
</ul>

@if (Model.Any())
{
    <table class="table table-striped">
        @foreach (var p in Model)
        {
            <tr>
                <td>@p.FirstName</td>
                <td>@p.LastName</td>
            </tr>
        }
    </table>
}