@model Catalyst.Web.Models.ViewModelBase
<!DOCTYPE html>
<!--[if IE 8 ]>    <html lang="en" class="no-js ie8 ie"> <![endif]-->
<!--[if IE 9 ]>    <html lang="en" class="no-js ie9 ie"> <![endif]-->
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Html.Partial("_MetaInfo", Model.Meta)
    @(Bundle.Css()
        .Add("~/Content/bootstrap.min.css")
        .MvcRender("~/Content/cache/combined_#.css"))
</head>
<body>
@Html.Partial("_Header")

<div>
    @RenderBody()
</div>

@(Bundle.JavaScript().Add("~/Scripts/bootstrap.min.js").Add("~/Scripts/jquery-2.2.2.min.js").MvcRender("~/Scripts/cache/combined_#.js"))
</body>
</html>
